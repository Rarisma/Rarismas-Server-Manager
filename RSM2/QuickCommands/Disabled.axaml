<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RSM2.QuickCommands.Disabled">

	<StackPanel>
		<TextBlock Text="             RSM cannot send commands directly to this server&#x0a;This may be because of how the server is designed/programmed." HorizontalAlignment="Center"/>
		<Border Height="50"/>
		<Button Content="Close Server" HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" Width="160"/>
		<Border Height="50"/>

		<Button Content="Save"		HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" Width="160"/>

	</StackPanel>
	
</UserControl>
